{
  "name": "x-star-design",
  "version": "0.0.55",
  "description": "A react component library developed by turingstar",
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build && sass src:dist --no-source-map",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,scss}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev",
    "test": "TZ=Asia/Shanghai jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.24.1",
    "@codemirror/autocomplete": "^6.9.0",
    "@codemirror/lang-cpp": "^6.0.2",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/lang-python": "^6.1.3",
    "@codemirror/language": "^6.9.0",
    "@codemirror/lint": "^6.4.0",
    "@uiw/codemirror-theme-bbedit": "^4.21.9",
    "@uiw/codemirror-theme-eclipse": "^4.21.9",
    "@uiw/codemirror-theme-okaidia": "^4.21.9",
    "@uiw/codemirror-theme-vscode": "^4.21.9",
    "@uiw/react-codemirror": "^4.21.9",
    "ahooks": "^3.7.8",
    "classnames": "^2.3.2",
    "randomcolor": "^0.6.2",
    "react-error-boundary": "^4.0.13",
    "react-sortable-hoc": "^2.0.0",
    "react-window": "^1.8.9",
    "x-star-utils": "^0.0.9"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.3.6",
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jest/globals": "^29.7.0",
    "@rc-component/context": "^1.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-window": "^1.8.5",
    "@umijs/lint": "^4.0.0",
    "antd": "^5.16.4",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "dayjs": "^1.11.10",
    "dumi": "^2.2.0",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "rc-resize-observer": "^1.4.0",
    "rc-table": "^7.45.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^6.1.0"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=4.7.0",
    "@rc-component/context": ">=1.3.0",
    "antd": ">=4.23.0",
    "dayjs": ">=1.0.0",
    "rc-resize-observer": ">=1.0.0",
    "rc-table": ">=7.26.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "XYD Tech Team tech@xinyoudui.com"
  ]
